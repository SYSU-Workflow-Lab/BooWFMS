    <state id="first">
        <onentry>
            <log label="temp lable" expr="'into first'"/>
        </onentry>
        <transition event="test" target="testState"/>
    </state>

    <state id="testState">
        <onentry>
            <foreach array="_event.data" item="kvp">
                <log label="KEY" expr="kvp.getKey()"/>
                <log label="VALUE" expr="kvp.getValue()"/>
